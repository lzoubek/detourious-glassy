
group {
	name: "modules/efm_nav/main";

	parts {
		part {
			name: "base";
			type: RECT;
			mouse_events: 0;

			description {
				state: "default" 0.0;
				color: 0 0 0 0;
			}
		}

		part {
			name: "e.swallow.buttons";
			type: SWALLOW;

			description {
				state: "default" 0.0;
				align: 1.0 0.5;

				rel1 { 
					to: "base";
					relative: 0.0 0.0; 
					offset: 8 0; 
				} 
			}
		}
	}
}

group {
	name: "modules/efm_nav/back";

	parts {
		part {
			name: "base";
			type: IMAGE;
			mouse_events: 0;

			description {
				state: "default" 0.0;
				max: 29 24;
				min: 29 24;
				fixed: 1 1;
				color: 255 255 255 255;

				image {
					normal: "images/efmnav_bt0.png";
					border: 7 1 7 7;
				}
			}
		}

		part {
			name: "button";
			type: IMAGE;
			mouse_events: 1;
			repeat_events: 1;

			description {
				state: "default" 0.0;

				rel1 { 
					to: "base";
					relative: 0.0 0.0; 
					offset: 2 1; 
				} 

				rel2 { 
					to: "base";
					relative: 1.0 1.0; 
					offset: -1 -2; 
				}

				image {
					normal: "images/efmnav_bt1.png";
					border: 7 1 6 6;
				}
			}

			description {
				state: "pressed" 0.0;
				inherit: "default" 0.0;
				color: 255 255 255 192;
			}

			description {
				state: "disabled" 0.0;
				inherit: "default" 0.0;
				color: 255 255 255 128;
			}
		}

		part {
			name: "symbol";
			type: IMAGE;
			mouse_events: 0;

			description {
				state: "default" 0.0;
				max: 11 8;
				min: 11 8;
				fixed: 1 1;
				align: 0.5 0.5;
				color: 158 158 158 255;

				rel1 { 
					to: "button";
					relative: 0.0 0.0; 
					offset: 0 0; 
				} 

				rel2 { 
					to: "button";
					relative: 1.0 1.0; 
					offset: -1 -1; 
				}

				image {
					normal: "images/efmnav_bt6.png";
				}
			}

			description {
				state: "pressed" 0.0;
				inherit: "default" 0.0;
				color: 158 158 158 192;
			}

			description {
				state: "disabled" 0.0;
				inherit: "default" 0.0;
				color: 158 158 158 128;
			}
		}
	}

	programs {
		efm_navigation();
	}
}

