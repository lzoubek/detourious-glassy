
group {
	name: "e/modules/everything/list";
	parts {
		part {
			name: "list_frame";
			type: RECT;
			description {
				state: "default" 0.0;
				visible: 0;
			}
		}
		part {
			name: "base";
			type: RECT;
			description {
				state: "default" 0.0;
				rel1 {
					relative: 0.0 1.0;
					to_x: "list_frame";
					to_y: "input_base";
				}
				rel2 {
					to: "list_frame";
				}
			}
		}
		part {
			name: "input_base";
			type: RECT;
			description {
				state: "default" 0.0;
				rel2.relative: 1.0 0.0;
				rel2.offset: -1 23;
			}
		}
		part {
			name: "input_bg";
			clip_to: "input_base";
			
			description {
				state: "default" 0.0;
				/* rel1.offset: 0 ; */
				rel1.to: "input_base";
				rel2.to: "input_base";

				image.normal: "images/menubar.png";
			}
		}
		/* part {
		 * 	name: "input_bg_hilight";
		 * 	mouse_events: 0;
		 * 	type: RECT;
		 * 	description {
		 * 		state: "default" 0.0;
		 * 		color: 245 245 245 255;
		 * 		rel1.to: "input_bg";
		 * 		rel2.to: "input_bg";
		 * 		rel2.relative: 1.0 0.0;
		 * 		rel1.offset: 0 -1;
		 * 		rel2.offset: -1 -1;
		 * 	}
		 * } */

		part {
			name: "e.text.label";
			type: TEXT;
			effect: SHADOW;
			scale: 1;
			clip_to: "input_base";
			description {
				state: "default" 0.0;
				align: 0.0 0.5;
				rel1 {
					to: "input_bg";
					relative: 0.0 0.5;
					offset: 10  2;
				}
				rel2 {
					to: "input_bg";
					relative: 0.0 0.5;
					offset: 10  0;
				}
				TEXT_COLOR;				
				TEXT_LARGE("everything_typebuf", 0.0 0.5, 1 1);
			}
		}
		part {
			name: "cursor";
			mouse_events: 0;
			description {
				state: "default" 0.0;
				min: 10 11;
				max: 10 11;
				fixed: 1 1;
				rel1 {
					relative: 1.0  0.0;
					offset: 2    0;
					to: "e.text.label";
				}
				rel2 {
					relative: 1.0  1.0;
					offset: 12    -5;
					to: "e.text.label";
				}
				image {
					normal: "images/menu_im3.png";
				}
				color: 255 255 255 200;
			}
			description {
				state: "faded" 0.0;
				inherit: "default" 0.0;
				color: 255 255 255 50;
			}
		}
		part {
			name: "list_clip";
			type: RECT;
			clip_to: "base";
			description {
				state: "default" 0.0;
				color: 255 255 255 255;
				fixed: 1 1;
				rel1 {
					relative: 0.0 0.0;
					offset: 0 0;
					to: "base";
				}
				rel2 {
					relative: 1.0 0.0;
					offset: -1  -1;
					to_y: "bar_base";
					to_x: "base";
				}
			}
		}
		/* part {
		 * 	name: "list_bg";
		 * 	clip_to: "base";
		 * 	description {
		 * 		state: "default" 0.0;
		 * 		image.normal: "images/dialog_im0.png";
		 * 		rel1 {
		 * 			to: "list_clip";
		 * 			offset: 0 0;
		 * 		}
		 * 		/\* rel2 {
		 * 		 * 	/\\*to: "list_clip"; *\\/
		 * 		 * 	offset: -1  -1;
		 * 		 * } *\/
		 * 	}
		 * } */

		/* FIXME: brighten image instead */
		part {
			name: "list_bg";
			clip_to: "base";
			type: RECT;
			description {
				state: "default" 0.0;
				rel1.to: "list_clip";
				rel2.to: "list_clip";
				color: 245 245 245 255;
			}
		}
		part {
			name: "e.swallow.list";
			type: SWALLOW;
			clip_to: "list_clip";
			mouse_events: 1;
			description {
				state: "default" 0.0;
				rel1.to: "list_clip";
				rel2.to: "list_clip";
			}
			description {
				state: "default" 1.0;
				inherit: "default" 0.0;
				rel1.relative: -1.0 0.0;
				rel2.relative: 0.0 1.0;
			}
		}
		part {
			name: "e.swallow.list2";
			type: SWALLOW;
			clip_to: "list_clip";
			mouse_events: 1;
			description {
				state: "default" 0.0;
				rel1.to: "list_clip";
				rel2.to: "list_clip";
				rel1.relative: 1.0 0.0;
				rel2.relative: 2.0 1.0;
			}
			description {
				state: "default" 1.0;
				inherit: "default" 0.0;
				rel1.relative: 0.0 0.0;
				rel2.relative: 1.0 1.0;
			}
		}
		part {
			name: "list_top_shadow";
			mouse_events: 0;
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 40;
				rel1.to: "list_bg";
				rel2.to: "list_bg";
				rel2.relative: 1.0 0.0;
				rel1.offset: 0 -1;
				rel2.offset: -1 -1;
			}
		}
		part {
			name: "list_top_shadow2";
			mouse_events: 0;
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 10;
				rel1.to: "list_bg";
				rel2.to: "list_bg";
				rel2.relative: 1.0 0.0;
				rel1.offset: 0 0;
				rel2.offset: -1 0;
			}
		}

		part {
			name: "list_bottom_shadow";
			mouse_events: 0;
			description {
				state: "default" 0.0;
				rel1.relative: 0.0 1.0;
				rel1.offset: 0 -3;
				rel2.offset: -1 -1;
				rel1.to: "list_clip";
				rel2.to: "list_clip";

				image.normal: "images/bevel_evry_fade.png";
			}
		}
		part {
			name: "bar_base";
			type: RECT;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				visible: 0;
				rel1 {
					relative: 0.0 1.0;
					to: "base";
					offset: 0 -26;
				}
				rel2 {
					relative: 1.0 1.0;
					to: "base";
					offset: -1 -1;
				}
			}
		}
		part {
			name: "bar_clip";
			type: RECT;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				rel1 {
					to: "bar_base";
					offset: 0 0;
				}
				rel2 {
					to: "bar_base";
					offset: 3 -1;
				}
			}
			description {
				state: "faded" 0.0;
				inherit: "default" 0.0;
				color: 255 255 255 0;
			}
		}
		part {
			name: "bar_bg";
			clip_to: "base";
			mouse_events: 0;
			type: RECT;
			description {
				state: "default" 0.0;
				fixed: 1 1;
				rel1.to: "bar_base";
				rel2.to: "bar_base";
				color: 0 0 0 50;
			}
		}
		part {
			name: "bar_bg2";
			mouse_events: 0;
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 90;
				rel1.to: "bar_bg";
				rel2.to: "bar_bg";
				rel2.relative: 1.0 0.0;
				rel2.offset: -1 0;                
			}
		}
		part {
			name: "bar_bg_shadow";
			mouse_events: 0;
			description {
				state: "default" 0.0;
				/* color: 255 255 255 80; */
				rel1.to: "bar_bg";
				rel2.to: "bar_bg";
				image {
					normal: "images/bevel_tab_shadow.png";
				}
			}
		}
		part {
			name: "e.swallow.bar";
			clip_to: "bar_clip";
			type: SWALLOW;
			description {
				state: "default" 0.0;
				rel1.to: "bar_clip";
				rel1.offset: -1 0;
				rel2.to: "bar_clip";
				rel2.offset: -1 -1;
			}
		}
	}
	programs {
		program {
			name: "blink_off";
			signal: "show";
			source: "";
			action: STATE_SET "faded" 0.0;
			transition: SINUSOIDAL 0.6;
			target: "cursor";
			after: "blink_on";
		}
		program {
			name: "blink_on";
			action: STATE_SET "default" 0.0;
			transition: SINUSOIDAL 0.35;
			target: "cursor";
			after: "blink_off";
		}
		program {
			name: "show_list";
			signal: "e,state,list_show";
			source: "e";
			action: STATE_SET "visible" 0.0;
			target: "base";
		}
		program {
			name: "update_list";
			signal: "e,signal,update";
			source: "e";
			action: STATE_SET "faded" 0.0;
			/* transition: SINUSOIDAL 0.5; */
			target: "bar_clip";
			after: "update_list2";
		}
		program {
			name: "update_list2";
			action: STATE_SET "default" 0.0;
			/* transition: SINUSOIDAL 0.15; */
			target: "bar_clip";
		}

		program {
			name: "slide_left";
			signal: "e,action,slide,left";
			source: "e";
			action: STATE_SET "default" 0.0;
			target: "e.swallow.list";
			target: "e.swallow.list2";
			after: "slide_left2";
		}
		program {
			name: "slide_left2";
			action: STATE_SET "default" 1.0;
			transition: SINUSOIDAL 0.2;
			target: "e.swallow.list";
			target: "e.swallow.list2";
			after: "left_end";
		}
		program { name: "left_end";
			action: SIGNAL_EMIT "e,action,slide,left,finished" "";
		}
		program {
			name: "slide_right";
			signal: "e,action,slide,right";
			source: "e";
			action: STATE_SET "default" 1.0;
			target: "e.swallow.list";
			target: "e.swallow.list2";
			after: "slide_right2";
		}
		program {
			name: "slide_right2";
			action: STATE_SET "default" 0.0;
			transition: SINUSOIDAL 0.2;
			target: "e.swallow.list";
			target: "e.swallow.list2";
			after: "right_end";
		}
		program { name: "right_end";
			action: SIGNAL_EMIT "e,action,slide,right,finished" "";
		}
		program {
			name: "slide_default";
			signal: "e,action,slide,default";
			source: "e";
			action: STATE_SET "default" 0.0;
			target: "e.swallow.list";
			target: "e.swallow.list2";
		}
	}
}
group {
	name: "e/modules/everything/main";
	parts {
		part {
			name: "bg";
			mouse_events: 0;;
			type: RECT;
			description {
				state: "default" 0.0;
				color: 235 235 235 255;
			}
		}

		part {
			name: "win";
			type: RECT;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				color: 170 170 170 255;
				rel2.relative: 1.0 0.0;
				rel1.offset:  1 1;
				rel2.offset: -1 48;
			}
		}
		part {
			name: "list";
			source: "e/modules/everything/list";
			type: GROUP;
			description {
				state: "default" 0.0;
				visible: 1;
				min: 300 155;
				rel1 {
					to_y: "win";
					offset: 1 -1;
					relative: 0.0 1.0;
				}
				rel2 {
					offset: -2 -2;
				}
			}
		}
		part {
			name: "separator";
			mouse_events: 0;
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 75;
				rel1.to: "win";
				rel2.to: "win";
				rel1.relative: 0.0 1.0;
				rel1.offset: 0 -2;
				rel2.offset: -1 -2;
			}
		}
		part {
			name: "separator2";
			mouse_events: 0;
			type: RECT;
			description {
				state: "default" 0.0;
				color: 255 255 255 148;
				rel1.to: "win";
				rel2.to: "win";
				rel1.relative: 0.0 1.0;
				rel1.offset: 0 -1;
			}
		}
		part {
			name: "selector_frame";
			type: RECT;
			description {
				state: "default" 0.0;
				rel1.to: "win";
				rel2.to: "win";
			}
		}
		part {
			name: "subject_selector";
			type: GROUP;
			source: "e/modules/everything/selector_item";
			clip_to: "selector_frame";
			description {
				state: "default" 0.0;
				rel1.relative: 0.0 0.0;
				rel1.to: "selector_frame";
				rel2.relative: 0.5 1.0;
				rel2.to_y: "selector_frame";
			}
			description {
				state: "wide" 0;
				inherit: "default" 0.0;
				visible: 0;
				rel1.relative: -0.5 0.0;
				rel2.relative: 0.0 1.0;
			}
		}
		part {
			name: "action_selector";
			type: GROUP;
			source: "e/modules/everything/selector_item";
			clip_to: "selector_frame";
			description {
				state: "default" 0.0;
				rel1.relative: 0.5 0.0;
				rel1.to: "selector_frame";
				rel2.relative: 1.0 1.0;
				rel1.offset: -1 0;                
				rel2.to: "selector_frame";
			}
			description {
				state: "wide" 0;
				inherit: "default" 0.0;
				rel1.relative: 0.0 0.0;
				rel2.relative: 0.5 1.0;
			}
		}
		part {
			name: "object_selector";
			type: GROUP;
			source: "e/modules/everything/selector_item";
			clip_to: "selector_frame";
			description {
				state: "default" 0.0;
				visible: 0;
				rel1.relative: 1.0 0.0;
				rel1.to: "selector_frame";
				rel2.relative: 1.5 1.0;
				rel2.to: "selector_frame";
			}
			description {
				state: "wide" 0;
				inherit: "default" 0.0;
				visible: 1;
				rel1.relative: 0.5 0.0;
				rel2.relative: 1.0 1.0;
			}
		}
		/* part {
		 *     name: "selector_over2";
		 *     mouse_events: 0;
		 *     description {
		 *         state: "default" 0.0;
		 *         color: 255 255 255 90;
		 *         rel1.to: "selector_frame";
		 *         rel2.to: "selector_frame";
		 *         image {
		 *             normal: "images/bevel_under.png";
		 *         }
		 *     }
		 * } */
		part {
			name: "selector_over1";
			mouse_events: 0;
			description {
				state: "default" 0.0;
				color: 255 255 255 100;
				rel1.to: "selector_frame";
				rel2.to: "selector_frame";
				image {
					normal: "images/bevel_over.png";
				}
			}
		}
		part {
			name: "top";
			mouse_events: 0;
			type: RECT;
			description {
				state: "default" 0.0;
				color: 255 255 255 110;
				rel1.to: "win";
				rel2.to: "win";
				rel1.relative: 0.0 0.0;
				rel2.relative: 1.0 0.0;
				rel1.offset: 0 0;
				rel2.offset: -1 0;
			}
		}
		part {
			name: "bg_frame";
			mouse_events: 0;
			description {
				state: "default" 0.0;
				color: 85 85 85 128;
				image {
					normal: "images/menu_im0.png";
					border: 1 1 1 1;
					middle: 0;
				}
			}
		}
	}
	programs {
		program {
			name: "object_selector_hide";
			signal: "e,state,object_selector_hide";
			source: "e";
			action: STATE_SET "default" 0.00;
			transition: SINUSOIDAL 0.15;
			target: "object_selector";
			target: "subject_selector";
			target: "action_selector";
		}
		program {
			name: "object_selector_show";
			signal: "e,state,object_selector_show";
			source: "e";
			action: STATE_SET "wide" 0.00;
			transition: SINUSOIDAL 0.2;
			target: "object_selector";
			target: "subject_selector";
			target: "action_selector";
		}
	}
}
group {
	name: "e/modules/everything/selector_item";
	parts {
		part {
			name: "clip";
			type: RECT;
			mouse_events: 0;
			description {
				state: "default" 0.0;
			}
		}
		part {
			name: "bg";
			mouse_events: 0;
			type: RECT;
			description {
				state: "default" 0.0;
				color: 190 190 190 0;
			}
			description {
				state: "selected" 0.0;
				inherit: "default" 0.0;
				color: 205 205 205 50;
			}
		}
		part {
			name: "separator";
			mouse_events: 0;
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 60;
				rel1.relative: 1.0 0.0;
				rel2.offset: 0 -2;
				/* rel2.offset: -1 -2; */
			}
			description {
				state: "selected" 0.0;
				inherit: "default" 0.0;
				color: 0 0 0 65;
			}
		}
		part {
			name: "icon_clip";
			type: RECT;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				rel1.offset: 4 1;
				align: 0.0 0.5;
				min: 44 44;
				max: 44 44;
				fixed: 1 1;
				color: 235 235 235 255;
			}
			description {
				state: "selected" 0.0;
				inherit: "default" 0.0;
				color: 255 255 255 255;
			}
		}
		part {
			name: "e.swallow.icons";
			type: SWALLOW;
			clip_to: "icon_clip";
			mouse_events: 0;
			description {
				state: "default" 0.0;
				aspect: 1.0 1.0;
				align: 0.5 0.5;
				aspect_preference: BOTH;
				rel1.to: "icon_clip";
				rel2.to: "icon_clip";                
			}
		}
		part {
			name: "e.swallow.thumb";
			type: SWALLOW;
			clip_to: "icon_clip";
			mouse_events: 0;
			description {
				state: "default" 0.0;
				aspect: 1.0 1.0;
				align: 0.5 0.5;
				aspect_preference: BOTH;
				rel1.to: "icon_clip";
				rel2.to: "icon_clip";                
			}
		}
		part {
			name: "e.text.label";
			type: TEXT;
			effect: SHADOW;
			mouse_events: 0;
			clip_to: "clip";
			scale: 1;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				align: 0.0 1.0;
				fixed: 0 1;
				rel1.to:   "e.swallow.icons";
				rel1.relative:  1.0 0.0;
				rel2.to:   "clip";
				rel1.offset:   6 3;
				rel2.offset:   -6 -9;
				color: 60 60 60 255;
				color3: 240 240 240 128;
				TEXT_LARGE("everything_selector", 0.0 1.0, 0 1);
			}
			description {
				state: "selected" 0.0;
				inherit: "default" 0.0;
				color: 20 20 20 255;
			}
		}
		part {
			name: "e.text.plugin";
			type: TEXT;
			effect: SHADOW;
			mouse_events: 0;
			scale: 1;
			description {
				state: "default" 0.0;
				align: 0.0 0.0;
				rel1.to: "e.swallow.icons";
				rel1.relative:  1.0 0.0;
				rel1.offset: 6 8;
				rel2.offset: -6 -7;
				color: 80 80 80 255;
				color3: 240 240 240 128;
				TEXT_SMALL("label_small", 0.0 0.0, 0 1);
			}
			description {
				state: "selected" 0.0;
				inherit: "default" 0.0;
				color: 40 40 40 255;
				color3: 240 240 240 128;
			}
		}
		part {
			name: "over";
			mouse_events: 0;
			description {
				state: "default" 0.0;
				color: 255 255 255 150;
				rel2.offset: -3 -1;
				visible: 0;
				image {
					normal: "images/bevel_over.png";
				}
			}
			description {
				state: "selected" 0.0;
				inherit: "default" 0.0;
				/* visible: 1; */
			}
		}

		part {
			name: "event";
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 0;
			}
		}
	}
	programs {
		program {
			name: "sel";
			signal: "e,state,selected";
			source: "e";
			action: STATE_SET "selected" 0.0;
			target: "bg";
			/* target: "separator";
			 * target: "over"; */
