group {
	name: "e/modules/everything/thumbview/main/window";
	parts {
		part {
			name: "clip";
			type: RECT;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				color: 255 255 255 0;
			}
			description {
				state: "visible" 0.0;
				color: 255 255 255 255;
			}
		}
		part {
			name: "e.swallow.list";
			type: SWALLOW;
			clip_to: "clip";
			description {
				state: "default" 0.0;
				rel1 {
					relative: 0.0 0.0;
					offset: 6   1;
				}
				rel2 {
					relative: 1.0 1.0;
					offset: -3  -3;
				}
			}
		}
		part {
			name: "left_arrow";
			mouse_events: 0;
			scale: 1;
			description {
				state: "default" 0.0;
				visible: 0;
				image.normal: "images/menu_im5.png";
				aspect: 1.0 1.0;
				aspect_preference: VERTICAL;
				align: 0.0 0.5;
				min: 10 11;
				max: 10 11;
				color: 255 255 255 0;
			}
			description {
				state: "visible" 0.0;
				inherit: "default" 0.0;
				visible: 1;
				color: 255 255 255 255;
			}
		}
		part {
			name: "right_arrow";
			mouse_events: 0;
			scale: 1;
			description {
				state: "default" 0.0;
				visible: 0;
				image.normal: "images/menu_im3.png";
				aspect: 1.0 1.0;
				aspect_preference: VERTICAL;
				align: 1.0 0.5;
				min: 10 11;
				max: 10 11;
				color: 255 255 255 0;
			}
			description {
				state: "visible" 0.0;
				inherit: "default" 0.0;
				visible: 1;
				color: 255 255 255 255;
			}
		}
		part {
			name: "event";
			repeat_events: 1;
			type: RECT;
